{
  "name": "chocolate-frontend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/chocolate-frontend/src",
  "projectType": "application",
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*"
    ]
  },
  "targets": {
    "prebuild": {
      "command": "bash apps/chocolate-frontend/scripts/prebuild.sh fetch:contract",
      "outputPath": "apps/chocolate-frontend/src/assets/contract",
      "outputs": [
        "apps/chocolate-frontend/src/assets/contract"
      ],
      "inputs": []
    },
    "build": {
      "executor": "@nrwl/vite:build",
      "inputs": [
        "default"
      ],
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/chocolate-frontend"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      },
      "dependsOn": [
        "prebuild"
      ]
    },
    "serve": {
      "executor": "@nrwl/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "chocolate-frontend:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "chocolate-frontend:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "chocolate-frontend:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nrwl/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "chocolate-frontend:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "chocolate-frontend:build:development"
        },
        "production": {
          "buildTarget": "chocolate-frontend:build:production"
        }
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": [
        "coverage/apps/chocolate-frontend"
      ],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/chocolate-frontend"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/chocolate-frontend/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "mock-server": {
      "executor": "nx:run-commands",
      "options": {
        "command": "json-server --watch scripts/generate.cjs",
        "cwd": "apps/chocolate-frontend"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "netlify deploy",
        "cwd": "apps/chocolate-frontend"
      },
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "default"
      ]
    }
  },
  "tags": []
}